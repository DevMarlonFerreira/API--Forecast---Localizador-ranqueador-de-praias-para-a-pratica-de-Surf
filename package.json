{
  "name": "forecast-api",
  "version": "1.0.0",
  "private": true,
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevMarlonFerreira/Forecast"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Marlon Ferreira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DevMarlonFerreira/Forecast/issues"
  },
  "homepage": "https://github.com/DevMarlonFerreira/Forecast#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.2",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/module-alias": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.5",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.6.1",
    "@types/pino": "^6.3.8",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.1",
    "nock": "^13.1.0",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.12",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.13",
    "pino": "^6.11.3",
    "swagger-ui-express": "^4.1.6"
  }
}
